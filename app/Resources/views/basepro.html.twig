<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>{% block title %}Click and Cheers Pro{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" type="text/css" href="{{ asset('css/bootstrap.css')}}">
             <link rel="stylesheet" type="text/css" href="{{ asset('css/bootstrap-switch.min.css')}}">
            <link rel="stylesheet" type="text/css" href="{{ asset('css/main.css')}}">
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <style type="text/css">
            @-moz-document url-prefix() {
                select{
                    -moz-appearance: none;
                    background: url("../../../img/arrow.png") no-repeat scroll right transparent;
                 background-size: 11px;
                 padding-right:10px;
                }
            }
        </style>
    </head>
    <body onmousemove="souris(event);">
        {% block navbar %}
            {% include '::navbarpro.html.twig' %}
        {% endblock navbar %}
        
        <div class="mainContainer">
        {% block body %}{% endblock %}
        </div>
        {% block footer %}
            {% include '::footer.html.twig' %}
        {% endblock footer %}
        
        {% block javascripts %}
            <script src="{{ asset('js/jquery-2.1.0.js') }}" type="text/javascript" /></script>
            <script src="{{ asset('js/bootstrap.min.js') }}" type="text/javascript" /></script>
            <script src="{{ asset('js/bootstrap-switch.min.js') }}" type="text/javascript" /></script>
            <script src="{{ asset('js/function.js') }}" type="text/javascript" /></script>
            <script src="{{ asset('js/function-pro.js') }}" type="text/javascript" /></script>
            <script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
            <script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
            <script src="{{ asset('js/autocomplete-adress.js') }}" type="text/javascript" /></script>
            <script src="{{ asset('js/modernizr.custom.js') }}" type="text/javascript" /></script>
            <script src="{{ asset('js/svgcheckbx.js') }}" type="text/javascript" /></script>
        {% endblock %}
        <script>
          (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
          (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
          m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
          })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

          ga('create', 'UA-65830084-1', 'auto');
          ga('send', 'pageview');
        </script>

    </body>
</html>
