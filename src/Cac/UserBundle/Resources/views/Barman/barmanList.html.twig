<p>Les barmans</p>
<div class="listBarman row">
	{% for barman in barmans %}
	
		<div class="barmanProperties col-xs-offset-1 col-xs-10 col-sm-offset-1 col-sm-10 col-md-offset-1 col-md-10">
			<div class="col-xs-6 col-sm-3 col-md-3">
				<p>Créé le 20/10/2015</p>
			</div>
			<div class="col-xs-6 col-sm-3 col-md-3">
				<p>{{barman.email}}</p>
			</div>
			<div class="col-xs-6 col-sm-3 col-md-3">
				<p class="deleteBarman" data-id="{{barman.id}}">Supprimer</p>
			</div>

			<div class="col-xs-6 col-sm-3 col-md-3">
				<p>{{barman.id}}</p>
			</div>
		</div>
	{% endfor %}
</div>
<div class="addBarman">
	<a href="{{ path('barman_register') }}"><p>+</p></a>
</div>
<div class="formBarman">
	<p>Ajouter un barman</p>
	{#{% render(controller("CacUserBundle:Barman:register")) %} #}
</div>
