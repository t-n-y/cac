<h1>Event</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Brief</th>
            <td>{{ entity.brief }}</td>
        </tr>
        <tr>
            <th>Description</th>
            <td>{{ entity.description }}</td>
        </tr>
        <tr>
            <th>Eventdate</th>
            <td>{{ entity.eventDate|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Startat</th>
            <td>{{ entity.startAt|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Endat</th>
            <td>{{ entity.endAt|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Createdat</th>
            <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Displaystartat</th>
            <td>{{ entity.displayStartAt|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Displayendat</th>
            <td>{{ entity.displayEndAt|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>
