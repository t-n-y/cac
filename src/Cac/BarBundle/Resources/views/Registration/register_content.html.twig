{% trans_default_domain 'FOSUserBundle' %}

<form action="{{ path('cac_barman_user_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
    {{ form_errors(form) }}
 
    {{ form_widget(form) }}
    <div class="mt3">
        <input type="submit" value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}" class="mbtn mbtn-large mbtn-shadow mbtn-salmon" />
    </div>
</form>
